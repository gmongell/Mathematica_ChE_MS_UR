  3.5080843162397256`*^9, 3.5080843473897257`*^9}}],

Cell[BoxData[
 RowBox[{"kb", " ", "T", " ", 
  RowBox[{"Log", "[", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[Beta]"}], " ", "\[Epsilon]"}]], "+", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[Beta]", " ", "\[Epsilon]"}]]}], ")"}], "N"], 
   "]"}]}]], "Output",
