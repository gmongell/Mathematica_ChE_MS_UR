        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[Beta]"}], "*", 
          RowBox[{"\[Epsilon]", "[", 
           RowBox[{"k", ",", "s"}], "]"}]}], "]"}]}]], 
      RowBox[{"\[DifferentialD]", "k"}]}]}], ",", 
    RowBox[{"Assumptions", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"s", ">", "0"}], ",", 
       RowBox[{"A", ">", "0"}], ",", 
       RowBox[{"Element", "[", 
        RowBox[{
