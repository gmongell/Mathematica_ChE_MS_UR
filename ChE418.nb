   3.50808598800671*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"S", "/", "n"}], "*", "kb"}], "=", 
  RowBox[{
   RowBox[{
    FractionBox[
     RowBox[{"-", "1"}], "2"], 
