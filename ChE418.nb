    RowBox[{"Log", "[", 
     FractionBox[
      RowBox[{"1", "+", "x"}], "2"], "]"}]}]}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{
  "To", " ", "get", " ", "there", " ", "from", " ", "here", " ", "we", " ", 
   "notice", " ", "that", " ", "the", " ", "result", " ", "from", " ", "the", 
   " ", "last", " ", "assignment", " ", "was", " ", "entropy", " ", "in", " ",
    "terms", " ", "of", " ", "x"}], ",", " ", 
  RowBox[{
