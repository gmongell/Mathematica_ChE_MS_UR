Cell["\<\
We know that this is the partition function for a single up and down spin \
system :\
\>", "Input",
 CellChangeTimes->{{3.50808574012471*^9, 3.50808576364771*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"Q1", "[", "T_", "]"}], "=", 
  RowBox[{
   RowBox[{"Exp", "[", 
    RowBox[{"\[Epsilon]", "*", "\[Beta]"}], "]"}], "+", 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", "\[Epsilon]"}], "*", "\[Beta]"}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.5080841877537255`*^9, 3.5080842231887255`*^9}, {
  3.5080842688347254`*^9, 3.508084313743726*^9}}],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{
    RowBox[{"-", "\[Beta]"}], " ", "\[Epsilon]"}]], "+", 
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{"\[Beta]", " ", "\[Epsilon]"}]]}]], "Output",
 CellChangeTimes->{{3.5080842148297253`*^9, 3.5080842237867255`*^9}, {
   3.508084270873726*^9, 3.508084334229726*^9}, 3.508084387460713*^9}]
}, Open  ]],

Cell["For N particles :", "Input",
 CellChangeTimes->{{3.5080857673067102`*^9, 3.50808577815771*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"QN", "[", 
   RowBox[{"V_", ",", "T_"}], "]"}], "=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"Q1", "[", "T", "]"}], ")"}], "^", "N"}]}]], "Input",
