Cell[BoxData[
 SuperscriptBox[
  RowBox[{"(", 
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "\[Beta]"}], " ", "\[Epsilon]"}]], "+", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"\[Beta]", " ", "\[Epsilon]"}]]}], ")"}], "N"]], "Output",
